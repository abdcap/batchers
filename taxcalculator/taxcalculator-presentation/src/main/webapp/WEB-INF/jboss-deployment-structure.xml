<?xml version="1.0" encoding="UTF-8"?>
<jboss-deployment-structure>
    <deployment>

        <exclusions>

            <module name="javax.ws.rs.api" />
            <module name="org.jboss.as.jaxrs" />

            <module name="org.jboss.resteasy.resteasy-atom-provider" />
            <module name="org.jboss.resteasy.resteasy-cdi" />
            <module name="org.jboss.resteasy.resteasy-jaxrs" />
            <module name="org.jboss.resteasy.resteasy-jaxb-provider" />
            <module name="org.jboss.resteasy.resteasy-jackson-provider" />
            <module name="org.jboss.resteasy.resteasy-jettison-provider" />
            <module name="org.jboss.resteasy.resteasy-cdi" />
            <module name="org.jboss.resteasy.resteasy-yaml-provider" />
            <module name="org.jboss.resteasy.resteasy-yaml-provider" />
            <module name="org.jboss.resteasy.resteasy-multipart-provider" />
            <module name="org.jboss.resteasy.async-http-servlet-30" />

            <module name="org.apache.httpcomponents" />
            <module name="org.jboss.as.weld" />
            <module name="org.jboss.weld" />
            <module name="org.jboss.as.ee" />
            <module name="org.jboss.as.ejb3" />
            <module name="org.jboss.as.jpa" />
            <module name="org.jboss.as.mail" />
            <module name="org.jboss.as.osgi" />
            <module name="org.jboss.as.remoting" />
            <module name="org.jboss.as.transactions" />
            <module name="org.jboss.as.webservices" />

            <module name="org.apache.log4j" />
            <module name="org.apache.commons.logging" />
            <module name="org.jboss.log4j.logmanager" />
            <module name="org.slf4j" />
            <module name="org.jboss.logging" />
        </exclusions>
        <dependencies>
            <module name="sun.jdk">
                <exports>
                    <include path="com/sun/image/codec/jpeg" />
                </exports>
            </module>
        </dependencies>
    </deployment>
</jboss-deployment-structure>